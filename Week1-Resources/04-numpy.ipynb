      "source": [
        "# Let's see what happens when we multiply a 2D array with\n",
        "# 1) a scalar\n",
        "# 2) a 2D array with the same shape\n",
        "\n",
        "arr = np.arange(9).reshape(3,3)\n",
        "print(arr,\"\\n-----\")\n",
        "\n",
        "print(arr*2,\"\\n-----\")  # Scalar multiplication\n",
        "print(arr*arr)          # Element-wise multiplication"
      ]
