      "outputs": [
        {
          "name": "stdout",
          "output_type": "stream",
          "text": [
            "[[0 1 2]\n",
            " [3 4 5]\n",
            " [6 7 8]] \n",
            "-----\n",
            "[0 1 2] \n",
            "-----\n",
            "[[ 0  1  4]\n",
            " [ 0  4 10]\n",
            " [ 0  7 16]]\n",
            "[0 1 2]\n"
          ]
        }
      ],
      "source": [
        "# The scalar multiplication is as expected, and the array multiplication\n",
        "# is element-wise, which is also expected. \n",
        "# What would happen when the above array is multiplied with a 1D array?\n",
        "arr2 = np.arange(3)\n",
        "print(arr,\"\\n-----\")\n",
        "print(arr2,\"\\n-----\")\n",
        "\n",
        "print(arr2*arr)\n",
        "#the arr is extended down by copying the values from 1st row"
      ]
